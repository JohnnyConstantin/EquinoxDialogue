{
  "name": "equinox-dialogue",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nlaha.github.io/EquinoxDialogue",
  "author": "Nathan Laha",
  "description": "A dialogue tree builder for Equinox Engine",
  "main": "./public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/joy": "^5.0.0-alpha.48",
    "@mui/material": "^5.10.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "electron": "^21.1.1",
    "flume": "patch:flume@^0.8.0#.yarn/patches/flume-npm-0.8.0-e9f24da865.patch",
    "gh-pages": "^4.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "reactflow": "^11.1.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "flume@^0.8.0": "patch:flume@npm%3A0.8.0#./.yarn/patches/flume-npm-0.8.0-e9f24da865.patch"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.nlaha.equinoxdialogue",
    "productName": "Equinox Dialogue",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "desktop_assets/icon.ico",
      "publisherName": "Nathan Laha"
    },
    "linux": {
      "target": "AppImage",
      "icon": "desktop_assets/icon.png",
      "category": "Development"
    },
    "mac": {
      "target": "dmg",
      "icon": "desktop_assets/icon.png",
      "category": "public.app-category.developer-tools"
    }
  }
}
